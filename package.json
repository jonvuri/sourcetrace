{
	"name": "sourcetrace",
	"version": "0.1.0",
	"description": "Generate a \"stacktrace\" into JavaScript source code, finding the lines that start blocks and statements containing a given source pattern or line.",
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "git://github.com/jrajav/sourcetrace.git"
	},
	"dependencies": {
		"esprima": "~1.2.2",
		"traverse": "~0.6.6",
		"lodash": "~2.4.1"
	},
	"devDependencies": {
		"mocha": "~1.19.0",
		"chai": "~1.9.1",
		"eslint": "~0.6.2"
	},
	"scripts": {
		"test": "node_modules/eslint/bin/eslint.js . && node_modules/mocha/bin/mocha --reporter spec test.js"
	},
	"keywords": [
